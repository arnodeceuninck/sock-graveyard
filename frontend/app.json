{
  "expo": {
    "name": "Sock Graveyard",
    "slug": "sock-graveyard",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.arnodece.socks",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a1f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arnodece.socks"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a1f"
      },
      "package": "com.arnodece.socks",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to upload sock images."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "152f4e71-0d36-47f9-bf9f-7c4fd512d0b4"
      }
    }
  }
}
